      "src/**/*.{js,jsx,mjs}",
      "!**/testData.js",
      "!**/store.js",
      "!**/serviceWorker.js",
      "!src/index.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
